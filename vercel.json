{
  "version": 2,
  "builds": [
    { "src": "java/jdkDownload.sh", "use": "@now/static-build", "config": { "maxLambdaSize": "50mb" } },
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["java/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "methods": ["POST"],
      "dest": "index.js"
    }
  ],
  "env": {
    "JAVA_HOME": "/vercel/path0/java/jdk"
  }
}
